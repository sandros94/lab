{
  "name": "@sandros94/lab",
  "version": "0.1.4",
  "description": "A personal collection of Nuxt and Nitro tools.",
  "repository": "sandros94/lab",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground --dotenv=../.env",
    "dev:build": "nuxi build playground --dotenv=../.env",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground --dotenv=../.env",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@nuxtjs/mdc": ">=0.11",
    "@unhead/vue": ">=1.11",
    "h3-valibot": ">=0.0.9",
    "ioredis": ">=5",
    "undio": ">=0.2",
    "unstorage": ">=1"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/mdc": {
      "optional": true
    },
    "@nuxtlabs/monarch-mdc": {
      "optional": true
    },
    "@vueuse/code": {
      "optional": true
    },
    "aws4fetch": {
      "optional": true
    },
    "consola": {
      "optional": true
    },
    "crossws": {
      "optional": true
    },
    "h3-valibot": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "monaco-editor": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "@nuxtjs/mdc": "^0.14.0",
    "@unhead/vue": "^1.11.20",
    "@vueuse/core": "^12.7.0",
    "aws4fetch": "^1.0.20",
    "consola": "^3.4.0",
    "crossws": "^0.3.4",
    "defu": "^6.1.4",
    "h3-valibot": "^0.0.9",
    "ioredis": "^5.5.0",
    "nypm": "^0.6.0",
    "pathe": "^2.0.3",
    "undio": "^0.2.0",
    "unstorage": "^1.15.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.3",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.1",
    "@nuxtlabs/monarch-mdc": "^0.5.0",
    "changelogen": "^0.6.1",
    "eslint": "^9.21.0",
    "monaco-editor": "^0.52.2",
    "nuxt": "^3.15.4",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.5.2",
  "resolutions": {
    "@sandros94/lab": "workspace:*",
    "typescript": "5.6.3",
    "vue-tsc": "2.1.6"
  }
}
