{
  "name": "@sandros94/lab",
  "version": "0.2.0",
  "description": "A personal collection of Nuxt and Nitro tools.",
  "repository": "sandros94/lab",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "docs:preview": "nuxt preview",
    "release": "pnpm lint && pnpm test && pnpm prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm lint && vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && nuxt typecheck playground && nuxt typecheck docs"
  },
  "peerDependencies": {
    "@nuxtjs/mdc": ">=0.18",
    "@unhead/vue": ">=1.11",
    "ioredis": ">=5",
    "undio": ">=0.2",
    "unstorage": ">=1"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/mdc": {
      "optional": true
    },
    "@nuxtlabs/monarch-mdc": {
      "optional": true
    },
    "@vueuse/code": {
      "optional": true
    },
    "aws4fetch": {
      "optional": true
    },
    "consola": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "monaco-editor": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.0",
    "@nuxtjs/mdc": "^0.19.1",
    "@standard-schema/spec": "^1.0.0",
    "@unhead/vue": "^2.0.19",
    "@vueuse/core": "^13.9.0",
    "aws4fetch": "^1.0.20",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "ioredis": "^5.8.2",
    "nypm": "^0.6.2",
    "pathe": "^2.0.3",
    "ufo": "^1.6.1",
    "undio": "^0.2.0",
    "unstorage": "^1.17.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.0",
    "@nuxt/test-utils": "^3.21.0",
    "@nuxtlabs/monarch-mdc": "^0.8.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "monaco-editor": "0.55.1",
    "monaco-yaml": "^5.4.0",
    "nuxt": "catalog:",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.2"
  },
  "packageManager": "pnpm@10.19.0",
  "resolutions": {
    "@sandros94/lab": "workspace:*",
    "dompurify": "^3.2.4",
    "node-forge": "^1.3.2"
  }
}
